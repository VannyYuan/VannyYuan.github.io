<% if(theme.fun_features.typing.enable && page.subtitle !== false){ %>
  <%- js_ex(theme.static_prefix.typed, "/typed.min.js") %>
  <script>
    var random_num = Number(1);
    var typed = new Typed('#subtitle', {
      strings: [
        '  ',
        "<%- data.subtitle %>&nbsp;",
      ],
      cursorChar: "<%- theme.fun_features.typing.cursorChar[Math.floor(Math.random()*10)] %>",
      typeSpeed: <%- theme.fun_features.typing.typeSpeed %>,
      loop: <%- theme.fun_features.typing.loop %>,
    });
    typed.stop();
    $(document).ready(function () {
      $(".typed-cursor").addClass("h2");
      typed.start();
    });
  </script>
<% } %>
